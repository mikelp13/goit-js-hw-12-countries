<h2 class="title">{{name}}</h2>
<div class="wrapper">
  <div class="info-container">
    <p class="text"><span class="text__item">Capital:</span> {{capital}}</p>
    <p class="text"><span class="text__item">Population:</span> {{population}}</p>
    <p class="text"><span class="text__item">Languages:</span></p>
    <ul class="languages-list">
      {{#each languages}}
      <li class="languages-list__item">{{this.name}}</li>
      {{/each}}
    </ul>
  </div>
    <img class="image" src="{{flag}}" alt="flag of {{name}}" width="400">
</div>